{
  "Routes": [
    {
      "DownstreamPathTemplate": "/booking-api/{url}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/booking-api/{url}",
      "ReRouteIsCaseSensitive": false,
      "downstreamHostAndPorts": [
        {
          "host": "localhost",
          "port": 7000
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "PeriodTimespan": 60,
        "Limit": 1000
      }
    },
    {
      "DownstreamPathTemplate": "/schedule-api/{url}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/schedule-api/{url}",
      "ReRouteIsCaseSensitive": false,
      "downstreamHostAndPorts": [
        {
          "host": "localhost",
          "port": 8000
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "PeriodTimespan": 60,
        "Limit": 2
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000",
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration",
    "ServiceDiscoveryProvider": {

      "Host": "localhost",
      "Port": "5000"
    }
  }
}

